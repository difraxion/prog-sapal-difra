			left -85px
			span
				right 14px
				padding 16px 0 16px 23px
				background-position 0 50%
				background-image url(../img/pp-larrows.png)
		.rc
			right -85px
			span
				left 7px
				padding 16px 23px 16px 0
				background-position 49px 50%
				background-image url(../img/pp-rarrows.png)
	#ul-listado-pp
		li
			list-style-type none
			margin 15px 0
			a
				background #2cb2ba
				padding 25px 0
				transition(all 0.3s ease-in-out)
				span
					color blanco
					brandond_bold(2em)
