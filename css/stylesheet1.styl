//----------------------------------- PROYECTOS PRIORITARIOS ----------------------------------------
//---------------------------------------------------------------------------------------------------
#pp
	#nav
		position relative
		width 928px
		margin 25px auto 0 auto
		background #2cb2ba
		border #e0e0e0 solid 1px
		overflow hidden
		h1
			color blanco
			text-transform uppercase
			brandond_bold(3em)
			padding 46px 100px
			margin 0!important
		.lc, .rc
			position absolute
			top 0
			height 100%
			width 170px
			background #f5f5f5
			border-radius 50%
			span
				position absolute
				top 50%
				margin-top -24px
				display inline-block
				color oscuro
				monserrat_regular(.7em)
				background-color transparent
				background-repeat no-repeat
